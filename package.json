{
  "name": "zen-svelte-ui",
  "version": "1.0.1",
  "description": "A UI Component Framework with Svelte",
  "author": "Nikita Lebedev",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "sirv-cli": "^0.4.0",
    "svelte": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:qmzik/Svelte-UI-framework.git"
  },
  "license": "MIT",
  "keywords": [
    "svelte",
    "ui",
    "ui-components",
    "component"
  ],
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev"
  }
}
